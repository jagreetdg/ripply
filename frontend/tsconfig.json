{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"jsx": "react-native",
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"],
	"exclude": ["ripply-waitlist/**/*"]
}
