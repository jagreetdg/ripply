{
	"expo": {
		"name": "frontend",
		"slug": "frontend",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "ripply",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#121212"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.ripply.app"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#121212"
			},
			"package": "com.ripply.app"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"@react-native-google-signin/google-signin",
				{
					"iosUrlScheme": "com.googleusercontent.apps.341363879666-aklnvkpa9lr5agvg8hhae5fh90ev3g5h"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"developmentClient": {
			"silentLaunch": true
		},
		"extra": {
			"EXPO_PUBLIC_API_URL": "https://ripply-backend.onrender.com",
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "7ff319e6-a30f-44d3-8f86-53f65616af8c"
			}
		}
	}
}
